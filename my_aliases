 MY ALIAS

# sys
alias c='clear'
alias e='exit'
#
alias ..='cd ..'
alias ..2='cd ../../'
alias ..3='cd ../../../'
alias ..4='cd ../../../../'
alias ..5='cd ../../../../../'

alias l.='ls -d .* --color=auto'
alias lla='ls -la'
alias h='history'
alias j='jobs -l'

alias path='echo -e ${PATH//:/\\n}'
alias now='date +"%T"'
alias nowtime=now
alias nowdate='date +"%d-%m-%Y"'

alias toali='vim ~/.bashrc'
alias setali='source ~/.bashrc'

# git
alias g='git'
alias stts='git status'
alias master='git checkout master'
alias pull='git pull'
alias push='git push'
alias pushu='git push --set-upstream'
alias pushuo='git push --set-upstream origin'
alias cho='git checkout'
alias newb='git branch'



# fca_hosts
alias uj='ssh usahsalvp301'
